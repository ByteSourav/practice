#include<stdio.h>
int main ()
{
    int marks;
    printf("Enter any marks:");
    scanf("%d",&marks);
    if (marks>=80 && marks <=100)
    {
        printf("A+");
    }
    else if (marks>=75 && marks<80)
    {
        printf("A");
    }
        else if (marks>=70 && marks<75)
        {
            printf("A-");
        }
        else if (marks>=65 && marks<70)
        {
            printf("B+");
        }
        else if (marks>=60 && marks<65)
        {
            printf("B");
        }
        else if (marks>=0 && marks<40)
        {
            printf("F");
        }
        else
        {
            printf("Enter a valid number");
        }
    return 0;
}
